{
  "outputDirectory": "dist",
  "builds": [
    { "src": "dist/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/dist/$1" }
  ],
  "alias": ["koiauction88.me", "fkoi88.me"],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "(?<host>.*)"
        }
      ],
      "destination": "https://koiauction88.me/$1",
      "statusCode": 301
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "(?<host>.*)"
        }
      ],
      "destination": "https://fkoi88.me/$1",
      "statusCode": 301
    }
  ]
}