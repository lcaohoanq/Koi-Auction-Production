{
  "version": 2,
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/(.*)", "destination": "/dist/$1" }
  ],
  "alias": ["fkoi88.me"],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "fkoi88.me"
        }
      ],
      "destination": "/$1",
      "statusCode": 200
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "(?<host>.*)"
        }
      ],
      "destination": "https://fkoi88.me/$1",
      "statusCode": 301
    }
  ]
}
